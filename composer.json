{
	"name": "qiuhuchun/captcha",
	"type": "package",
	"description": "Laravel 5 Captcha Package",
	"keywords": ["laravel5 Captcha", "laravel5 Security", "Captcha"],
	"homepage": "https://github.com/mewebstudio/captcha",
	"license": "MIT",
	"authors": [
		{
			"name": "Muharrem ERÄ°N",
			"email": "me@mewebstudio.com",
			"homepage": "https://github.com/mewebstudio",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.2.0",
		"ext-gd": "*",
		"illuminate/config": "~7.0",
		"illuminate/filesystem": "~7.0",
		"illuminate/support": "~7.0",
		"illuminate/hashing": "~7.0",
		"intervention/image": "~2.5"
	},
	"require-dev": {
		"phpunit/phpunit": "8.5-dev",
        "mockery/mockery": "1.3.x-dev"
	},
	"autoload": {
		"psr-4": {
			"Qiuhuchun\\Captcha\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		],
		"psr-4": {
			"Qiuhuchun\\Test\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"extra": {
		"laravel": {
			"providers": [
				"Qiuhuchun\\Captcha\\CaptchaServiceProvider"
			],
			"aliases": {
				"Captcha": "Qiuhuchun\\Captcha\\Facades\\Captcha"
			}
		}
	}
}
